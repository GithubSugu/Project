<h1>Listing teams</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Id</th>
    <th>Contactno</th>
	<th>EmailID</th>
	<th>ATTID</th>
	<th>DOB</th>
  </tr>

<% @blackflag.each do |team| %>
  <tr>
    <td><%=h team.Name %></td>
    <td><%=h team.EmpID %></td>
    <td><%=h team.ContactNum %></td>
	<td><%=h team.MailID%></td>
	<td><%=h team.ATTUID%></td>
	<td><%h team.DOB%></td>
  </tr>
<% end %>
</table>
